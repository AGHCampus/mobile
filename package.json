{
    "name": "aghCampusMap",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "react-native start",
        "ios": "react-native run-ios --simulator 'iPhone 14'",
        "android": "react-native run-android",
        "pod": "cd ios && pod install",
        "lint": "eslint .",
        "test": "jest",
        "clean-modules": "rm -rf node_modules",
        "clean-android": "android/gradlew -p android clean || true && rm -rf android/build android/app/build",
        "clean-ios": "rm -rf ios/build ios/Pods && rm -rf ~/Library/Developer/Xcode/DerivedData && bundle exec pod repo update",
        "clean-full": "yarn clean-modules && yarn clean-android && yarn clean-ios && watchman watch-del-all || true && rm -rf $TMPDIR/metro-* || true && yarn cache clean --all",
        "clean-and-ready": "yarn clean-full && yarn && yarn pod"
    },
    "dependencies": {
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0-0",
        "@babel/plugin-proposal-optional-chaining": "^7.0.0-0",
        "@babel/plugin-transform-arrow-functions": "^7.0.0-0",
        "@babel/plugin-transform-shorthand-properties": "^7.0.0-0",
        "@babel/plugin-transform-template-literals": "^7.0.0-0",
        "@gorhom/bottom-sheet": "^4",
        "@gorhom/portal": "^1.0.14",
        "@react-native-community/geolocation": "^3.0.6",
        "@react-navigation/bottom-tabs": "^6.5.7",
        "@react-navigation/native": "^6.1.6",
        "react": "18.2.0",
        "react-native": "0.71.6",
        "react-native-fast-image": "^8.6.3",
        "react-native-gesture-handler": "^2.9.0",
        "react-native-linear-gradient": "^2.7.3",
        "react-native-map-clustering": "^3.4.2",
        "react-native-maps": "^1.4.0",
        "react-native-pager-view": "^6.2.0",
        "react-native-reanimated": "^3.2.0",
        "react-native-safe-area-context": "^4.5.1",
        "react-native-screens": "^3.20.0",
        "react-native-tab-view": "^3.5.1",
        "react-navigation": "^4.4.4"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native-community/eslint-config": "^3.2.0",
        "@tsconfig/react-native": "^2.0.2",
        "@types/jest": "^29.2.1",
        "@types/react": "^18.0.24",
        "@types/react-test-renderer": "^18.0.0",
        "babel-jest": "^29.2.1",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.2.1",
        "metro-react-native-babel-preset": "0.73.9",
        "prettier": "^2.8.7",
        "react-test-renderer": "18.2.0",
        "typescript": "4.8.4"
    },
    "jest": {
        "preset": "react-native"
    }
}
